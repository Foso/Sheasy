                "operationId": "partialUpdate",
                "tags": [
                    "pet"
                ],
                "produces": [
                    "application/json",
                    "application/xml",
                    "text/plain",
                    "text/html"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "description": "ID of pet that needs to be fetched",
                        "name": "petId",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "body",
                        "description": "Pet object that needs to be added to the store",
                        "name": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Pet"
                        }
                    }
                ]
            }
        },
        "/pet/uploadImage": {
