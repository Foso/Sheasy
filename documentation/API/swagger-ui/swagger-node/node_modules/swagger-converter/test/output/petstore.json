                "produces": [
                    "application/json",
                    "application/xml",
                    "text/plain",
                    "text/html"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "description": "ID of pet that needs to be fetched",
                        "name": "petId",
                        "required": true,
                        "type": "integer",
                        "default": 1,
                        "maximum": 100000,
                        "minimum": 1
                    }
                ]
            },
            "delete": {
                "responses": {
                    "200": {
                        "description": "No response was specified"
                    },
                    "400": {
                        "description": "Invalid pet value"
                    }
                },
                "description": "",
